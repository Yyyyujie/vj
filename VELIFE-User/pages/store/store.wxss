}
.tab1{
  width: 33%;
  position: relative;
}
.tab1:last-child::after{
  width: 0;
}
.tab1::after{
  content: "";
  display: block;
  height: 100%;
  width: 1rpx;
  background: #f5f5f5;
  position: absolute;
  right: 0;
  top: 0;
}