}
.evaBox::before,.storeAd::before{
  content: "";
  display: block;
  width: 100%;
  height: 30rpx;
  background: #f5f5f5;
}
.evaBox .title,.storeAd .title,.address,.serverTitle,.serverBox{
  padding: 0 20rpx;
  box-sizing: border-box;
}
.hairdresser{
  margin-bottom: 60rpx;
}
.evaTab{
  width: 100%;
  height: 150rpx;
  display: flex;
  justify-content: space-around;
