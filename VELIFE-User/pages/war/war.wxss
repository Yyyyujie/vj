page{
  padding: 100rpx 80rpx 0;
  box-sizing: border-box;
}
.txBox{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  margin: 0 auto;
}
.txBox image{
  width: 100%;
  height: 100%;
}
.btn{
  margin-top: 100rpx;
  background: #E31E4C;
  color: #fff;
  font-size: 30rpx;
}
.title{
  font-size: 40rpx;
  font-weight: 500;
  letter-spacing: 8rpx;
  font-family: "Microsoft YaHei";
  margin:30rpx 0 10rpx;
  text-align: center;
}
.titleEng{
  color: #D71B4B;
  font-size: 24rpx;
  font-weight: bold;
  text-align: center;
}