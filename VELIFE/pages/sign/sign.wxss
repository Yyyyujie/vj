page{
  width: 100%;
  height: 100%;
}
.banner{
  width: 100%;
  height: 32%;
  position: relative;
}
.banner image{
  width: 100%;
  height: 100%;
}
.bot{
  width: 100%;
  height: 40rpx;
  position: absolute;
  left: 0rpx;
  bottom: -4rpx;
}
.bot image{
  width: 100%;
  height: 100%;
}
.bannerZi{
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  background: rgba(0,0,0,0.75);
}
.bannerZi image{
  width: 50%;
  height: 70%;
}
.main{
  width: 100%;
  padding: 20rpx 50rpx;
  box-sizing: border-box;
}
.calendar{
  width: 50rpx;
  height: 50rpx;
}
image{
  width: 100%;
  height: 100%;
}
.timeBox{
  display: flex;
  align-items: center;
  font-size: 28rpx;
}
.timeBox text{
  margin-left: 14rpx;
}
.who{
  font-size: 24rpx;
  margin: 10rpx 0;
}
.start,.end{
  font-size: 24rpx;
  padding-left: 30rpx;
  position: relative;
  height: 150rpx;
  margin-bottom: 10rpx;
}

.start::after,.end::after{
  content: "";
  display: block;
  width: 10rpx;
  height: 10rpx;
  background: #CDCDCD;
  position: absolute;
  top: 10rpx;
  left: 0;
  border-radius: 50%;
}
.start.active::after{
  background: #229BF6;
}
.end.active::after{
  background: #229BF6;
}
.end::before{
  content: "";
  display: block;
  width: 1px;
  height: 100%;
  background: #CDCDCD;
  position: absolute;
  left: 4rpx;
  top: -140rpx;
}
.signBtn{
  width: 300rpx;
  height: 300rpx;
  margin: 0 auto;
  position: relative;
}
.signBtn picker{
  width: 300rpx;
  height: 300rpx;
}
.signBtn image{
   width: 300rpx;
  height: 300rpx;
}
.signTime{
  color: #ccc;
  margin: 10rpx 0;
}
.ka{
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  text-align: center;
  line-height: 300rpx;
  color: #fff;
  font-size: 36rpx;
}
.address{
  width:24rpx;
  height:24rpx;
  display: inline-block;
}